{
  "name": "iron-ui",
  "version": "0.0.6",
  "description": "A react UI Library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "**/style/**",
    "*.less",
    "*.css"
  ],
  "author": "iron",
  "license": "MIT",
  "keywords": [
    "react",
    "antd",
    "virtualized",
    "select",
    "autocomplete",
    "tree"
  ],
  "homepage": "https://ui.chenggang.win",
  "scripts": {
    "build-es": "babel --env-name es --config-file ./babel.esm.js -d es/ components",
    "build-es-less": "lessc components/index.less es/index.css",
    "build": "rimraf es/ && yarn build-es",
    "dist": "webpack",
    "storybook": "start-storybook -p 6008",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
    "antd": "^3.26",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "react-virtualized": "^9.22.2"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.3",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpackbar": "^4.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "300 kB"
    }
  ]
}
